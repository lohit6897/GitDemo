package Practice;

public class big {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int arr[]= {13,2,5,14,5,22,6,9};
		int temp = 0;
		for(int i=0;i<arr.length;i++)
		{
			for(int j=i+1;j<arr.length;j++)
			{
				if(arr[i]<arr[j])
				{
				temp=arr[i];
				}
			}
		}
		System.out.println(temp);
		
	}

}
